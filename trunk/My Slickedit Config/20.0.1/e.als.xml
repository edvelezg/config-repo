<profile name="/language/e/aliases">
	<p n="=surround_with_if">
		<text>
			if (%\c) {
			%\m sur_text -indent%
			}
		</text>
	</p>
	<p n="=surround_with_switch">
		<text>
			switch (%\c) {
			%\m sur_text%
			}
		</text>
	</p>
	<p n="=surround_with_while">
		<text>
			while (%\c) {
			%\m sur_text -indent%
			}
		</text>
	</p>
	<p n="=surround_with_do...while">
		<text>
			do {
			%\m sur_text -indent%
			} while (%\c);
		</text>
	</p>
	<p n="=surround_with_case">
		<text>
			case %\c:
			%\m sur_text -indent%
			break;
		</text>
	</p>
	<p n="=surround_with_union">
		<text>
			union {
			%\m sur_text -indent%
			} %\c;
		</text>
	</p>
	<p n="=surround_with_say">
		<text>
			say('%\m sur_text -stripend ;% "'%\m sur_text -stripend ;%'"');
		</text>
	</p>
	<p n="=surround_with_struct">
		<text>
			struct {
			%\m sur_text -indent%
			} %\c;
		</text>
	</p>
	<p n="=surround_with_if...else">
		<text>
			if (%\c) {
			%\m sur_text -indent%
			} else {
			%\m sur_text -indent%
			}
		</text>
	</p>
	<p n="=surround_with_for">
		<text>
			for (%\c) {
			%\m sur_text -indent%
			}
		</text>
	</p>
	<p n="=surround_with_default">
		<text>
			default:
			%\m sur_text -indent%
		</text>
	</p>
	<p n="=surround_with_if_condition">
		<text>
			if (%\m sur_text -stripend ;%) {
			%\i%\c
			}
		</text>
	</p>
	<p n="=surround_with_braces">
		<text>
			{
			%\m sur_text -indent%
			}
		</text>
	</p>
</profile>

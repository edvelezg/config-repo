<options>
	<profile n="language.coffeescript" version="1">
		<p n="fileproject_default_profile" v="User"/>
	</profile>
	<profile n="language.coffeescript.fileproject_profiles.User" version="1">
		<p n="content">
			<Project>
				<Config Name="Release">
					<Menu>
						<Target Name="Compile" MenuCaption="&amp;Compile" CaptureOutputWith="ProcessBuffer" SaveOption="SaveCurrent" RunFromDir="%rw">
							<Exec CmdLine="coffee -c %f"/>
						</Target>
						<Target Name="Build" MenuCaption="&amp;Build" CaptureOutputWith="ProcessBuffer" SaveOption="SaveWorkspaceFiles" RunFromDir="%rw">
							<Exec/>
						</Target>
						<Target Name="Rebuild" MenuCaption="&amp;Rebuild" CaptureOutputWith="ProcessBuffer" SaveOption="SaveWorkspaceFiles" RunFromDir="%rw">
							<Exec/>
						</Target>
						<Target Name="Debug" MenuCaption="&amp;Debug" SaveOption="SaveNone" RunFromDir="%rw">
							<Exec/>
						</Target>
						<Target Name="Execute" MenuCaption="E&amp;xecute" SaveOption="SaveNone" RunFromDir="%rw">
							<Exec/>
						</Target>
					</Menu>
				</Config>
			</Project>
		</p>
	</profile>
</options>
